{
  "name": "rc-neumorphism",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "lint": "eslint ./src --",
    "clean-dist": "rimraf \"dist\"",
    "build": "yarn make-demos && yarn clean-dist && cross-env NODE_ENV=production webpack --config webpack/prod.config.js",
    "build-core": "yarn clean-dist && cross-env NODE_ENV=production webpack --config webpack/core.config.js",
    "dev": "cross-env NODE_ENV=development yarn make-demos && node dev-server",
    "deploy": "yarn build && now --prod",
    "build-neu-scss": "node-sass rc-neumorphism/src --output rc-neumorphism/lib",
    "build-neu-js": "babel --config-file rc-neumorphism/babel.config.js --out-dir rc-neumorphism/lib rc-neumorphism/src --verbose",
    "build-neu": "yarn rimraf \"rc-neumorphism/lib\" && yarn build-neu-js && yarn build-neu-scss",
    "make-icons": "node tools/make-icons",
    "make-demos": "node tools/make-demos"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenvanhoang26041994/rc-neumorphism.git"
  },
  "author": "Hoang Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nguyenvanhoang26041994/rc-neumorphism/issues"
  },
  "homepage": "https://github.com/nguyenvanhoang26041994/rc-neumorphism#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "rc-neumorphism": "1.7.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "workbox-window": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^1.0.2",
    "handlebars": "^4.7.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "ip": "^1.1.5",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0",
