{
  "name": "fork-boilerplate",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "fork-ui:lint-css": "stylelint fork-ui/components/Switch/*.scss",
    "fork-ui:lint-js": "eslint fork-ui/components/Switch/*.js",

    "lint": "eslint ./src --",
    "clean-dist": "rimraf \"dist\"",
    "clean-guide-dist": "rimraf \".guide/dist\"",
    "build": "yarn clean-dist && cross-env NODE_ENV=production webpack --config webpack/prod.config.js",
    "build-guide": "yarn clean-guide-dist && yarn guide && cross-env NODE_ENV=production webpack --config .guide/webpack/prod.config.js",
    "build-core": "yarn clean-dist && cross-env NODE_ENV=production webpack --config webpack/core.config.js",
    "dev": "cross-env NODE_ENV=development node .dev-server",
    "deploy-guide": "yarn build-guide && vercel --prod",
    "make-icons": "node .guide/cli/make-icons",
    "guide": "node .guide/cli/make-guide",
    "start-guide": "yarn guide && node .guide/server.js",
    "build-fork-scss": "node-sass fork-ui/src --output fork-ui/lib",
    "build-fork-js": "babel --config-file fork-ui/babel.config.js --out-dir fork-ui/lib fork-ui/src --verbose",
    "build-fork": "yarn rimraf \"fork-ui/lib\" && yarn build-fork-js && yarn build-fork-scss",
    "fork:lint": "eslint --config ./fork-ui/.eslintrc ./fork-ui/src",
    "submodule-fork-ui": "git submodule add https://github.com/nguyenvanhoang26041994/fork-ui.git",
    "submodule-tabler-icon": "git submodule add https://github.com/nguyenvanhoang26041994/tabler-icons.git",
    "init-all": "yarn submodule-fork-ui && yarn submodule-tabler-icon && yarn make-icons && yarn guide"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenvanhoang26041994/fork-boilerplate.git"
  },
  "author": "Hoang Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nguyenvanhoang26041994/fork-boilerplate/issues"
  },
  "homepage": "https://github.com/nguyenvanhoang26041994/fork-boilerplate#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "markdown": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^12.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.1.0",
    "workbox-window": "^4.3.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^1.0.2",
    "handlebars": "^4.7.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "url": "^0.11.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
